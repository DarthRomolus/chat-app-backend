generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model TestModel {
  id        String    @id @default(uuid())
  name      String
  value     Int       @default(0)
  createdAt DateTime  @default(now())
}
model Messages{
  id        String    @id @default(uuid())
  userID    String
  chatID    String
  data      String
  time      DateTime
}